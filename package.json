{
  "name": "musicapp",
  "version": "1.0.0",
  "description": "learn basics of piano",
  "main": "server/server.js",
  "scripts": {
    "start": "npm run all",
    "start-nodemon": "nodemon server/server.js",
    "compile-watch": "webpack --watch",
    "all": "npm run start-nodemon & npm run compile-watch",
    "webpack-prod": "npx webpack",
    "webpack-dev": "npx webpack --env=dev",
    "move": "cp ./client/static/index.html ./build/ && cp ./client/static/jquery-1.7.2.min.js ./build/ && cp ./client/static/styles.css ./build/",
    "build": "rm -rf ./build && mkdir build && webpack --mode production -p --progress --config webpack.production.config.js && npm run move",
    "deploy": "npm run build && aws s3 sync ./build s3://www.learnpianofun.com --acl public-read --profile default"
  },
  "author": "Jordan Daniels",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.205.0",
    "axios": "^0.17.1",
    "express": "^4.16.2",
    "firebase": "^4.9.0",
    "jquery": "^3.3.1",
    "re-base": "^3.2.2",
    "react": "^16.8.6",
    "react-device-detect": "^1.9.10",
    "react-dom": "^16.2.0",
    "react-medium-image-zoom": "^3.0.10",
    "react-popup": "^0.9.2",
    "react-redux": "^5.0.6",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-spinners": "^0.6.1",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.10.0",
    "redux-promise-middleware": "^5.0.0",
    "styled-components": "^4.3.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "babel-cli": "^6.26.0",
    "html-webpack-plugin": "^3.2.0",
    "jsdoc": "^3.5.5",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "webpack-cli": "^3.3.6",
    "@babel/core": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@blueprintjs/core": "^2.0.0-beta.3",
    "babel-loader": "^7.1.2",
    "css-loader": "^0.28.9",
    "babel-core": "^7.0.0-bridge.0",
    "file-loader": "^1.1.6",
    "url-loader": "^1.0.1",
    "webpack": "^4.38.0",
    "webpack-dev-server": "^2.11.1",
    "style-loader": "^0.19.1",
    "nodemon": "^1.14.11"
  }
}
